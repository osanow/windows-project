{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "react/prop-types": 0,
    "linebreak-style": "off",
    "react/jsx-one-expression-per-line": "off",
    "no-underscore-dangle": "off",
    "max-len": "off",
    "no-alert": "off",
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off"
  }
}
